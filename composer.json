{
    "license": "Apache-2.0",
    "description": "A simple class to log calls to instance/static methods.",
    "keywords": ["mocks", "debugging", "traceon", "runkit", "runkit7"],
    "type":    "library",
    "require": {
        "php": ">= 7.2",
        "ext-runkit7": ">=3.1.0a1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phan/phan": "^3.1.1"
    },
    "autoload": {
        "psr-4": {"TraceOn\\": "src/TraceOn"}
    },
    "autoload-dev": {
        "psr-4": {"TraceOn\\Tests\\": "tests"}
    }
}
